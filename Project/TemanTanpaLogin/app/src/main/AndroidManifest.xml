<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kominfo.go.id.temannologin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="kominfo.go.id.temannologin.MainActivity" />
        <activity
            android:name="kominfo.go.id.temannologin.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="kominfo.go.id.temannologin.view.InputActivity"
            android:parentActivityName="kominfo.go.id.temannologin.MainActivity"></activity>
    </application>

</manifest>